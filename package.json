{
  "private": true,
  "name": "ka-mensa-ui",
  "version": "0.0.0",
  "description": "Karlsruhe (KIT) Mensa meal plan UI for use with ka-mensa-api",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "lint": "tsc --noEmit -p tsconfig.lint.json && eslint --ignore-path .gitignore --ext .ts,.js,.vue ./src && stylelint \"./src/**/*.{css,vue}\"",
    "lint-fix": "tsc --noEmit -p tsconfig.lint.json && eslint --fix --ignore-path .gitignore --ext .ts,.js,.vue ./src && stylelint --fix \"./src/**/*.{css,vue}\"",
    "test": "npm run lint",
    "dev": "vite serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/ka-mensa-ui.git"
  },
  "keywords": [
    "karlsruhe",
    "mensa",
    "canteen",
    "user-interface"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/ka-mensa-ui/issues"
  },
  "homepage": "https://github.com/meyfa/ka-mensa-ui",
  "dependencies": {
    "@fontsource/nunito": "5.0.16",
    "moment": "2.30.1",
    "vue": "3.4.15"
  },
  "devDependencies": {
    "@meyfa/eslint-config": "5.1.0",
    "@types/events": "3.0.3",
    "@vitejs/plugin-vue": "5.0.3",
    "@vue/tsconfig": "0.5.1",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-plugin-jsdoc": "47.0.2",
    "eslint-plugin-vue": "9.19.2",
    "stylelint": "15.11.0",
    "stylelint-config-standard-vue": "1.0.0",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-plugin-pwa": "0.17.4",
    "vue-eslint-parser": "9.3.2"
  }
}
