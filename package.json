{
  "private": true,
  "name": "ka-mensa-ui",
  "version": "0.0.0",
  "description": "Karlsruhe (KIT) Mensa meal plan UI for use with ka-mensa-api",
  "scripts": {
    "build": "vite build",
    "lint": "tsc --noEmit -p tsconfig.lint.json && eslint --ignore-path .gitignore --ext .ts,.js,.vue ./src && stylelint \"./src/**/*.{css,vue}\"",
    "lint-fix": "tsc --noEmit -p tsconfig.lint.json && eslint --fix --ignore-path .gitignore --ext .ts,.js,.vue ./src && stylelint --fix \"./src/**/*.{css,vue}\"",
    "test": "npm run lint",
    "dev": "vite serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/ka-mensa-ui.git"
  },
  "keywords": [
    "karlsruhe",
    "mensa",
    "canteen",
    "user-interface"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/ka-mensa-ui/issues"
  },
  "homepage": "https://github.com/meyfa/ka-mensa-ui",
  "dependencies": {
    "@fontsource/nunito": "5.0.8",
    "events": "3.3.0",
    "moment": "2.29.4",
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@meyfa/eslint-config": "4.0.1",
    "@types/events": "3.0.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/tsconfig": "0.4.0",
    "cross-env": "7.0.3",
    "eslint": "8.46.0",
    "eslint-plugin-jsdoc": "46.4.3",
    "eslint-plugin-vue": "9.16.1",
    "stylelint": "15.10.2",
    "stylelint-config-standard-vue": "1.0.0",
    "typescript": "5.1.6",
    "vite": "4.4.8",
    "vite-plugin-pwa": "0.16.4",
    "vue-eslint-parser": "9.3.1"
  }
}
