{
  "name": "ka-mensa-ui",
  "version": "0.0.0",
  "description": "Karlsruhe (KIT) Mensa meal plan UI for use with ka-mensa-api",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./config/build/webpack.prod.js",
    "lint": "eslint --ignore-path .gitignore --ext .js,.vue . && stylelint ./src",
    "lint-fix": "eslint --fix --ignore-path .gitignore --ext .js,.vue . && stylelint --fix ./src",
    "test": "npm run lint",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./config/build/webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/ka-mensa-ui.git"
  },
  "keywords": [
    "karlsruhe",
    "mensa",
    "canteen",
    "user-interface"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/ka-mensa-ui/issues"
  },
  "homepage": "https://github.com/meyfa/ka-mensa-ui",
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "axios": "^0.21.1",
    "moment": "^2.29.1",
    "typeface-nunito": "^1.1.13",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "babel-loader": "^8.2.2",
    "babel-plugin-root-import": "^6.6.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^7.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0",
    "html-webpack-preconnect-plugin": "^1.2.0",
    "mini-css-extract-plugin": "^1.3.5",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.21.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^6.1.0"
  }
}
