{
  "private": true,
  "name": "ka-mensa-ui",
  "version": "0.0.0",
  "description": "Karlsruhe (KIT) Mensa meal plan UI for use with ka-mensa-api",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "lint": "tsc --noEmit -p tsconfig.lint.json && eslint . && stylelint \"./src/**/*.{css,vue}\"",
    "lint-fix": "tsc --noEmit -p tsconfig.lint.json && eslint --fix . && stylelint --fix \"./src/**/*.{css,vue}\"",
    "test": "npm run lint",
    "dev": "vite serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/ka-mensa-ui.git"
  },
  "keywords": [
    "karlsruhe",
    "mensa",
    "canteen",
    "user-interface"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/ka-mensa-ui/issues"
  },
  "homepage": "https://github.com/meyfa/ka-mensa-ui",
  "dependencies": {
    "@fontsource/nunito": "5.2.5",
    "moment": "2.30.1",
    "vue": "3.5.14"
  },
  "devDependencies": {
    "@meyfa/eslint-config": "8.0.1",
    "@typescript-eslint/parser": "8.39.1",
    "@vitejs/plugin-vue": "5.2.4",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.33.0",
    "eslint-plugin-vue": "9.33.0",
    "postcss-html": "1.8.0",
    "prettier": "3.6.2",
    "stylelint": "16.23.1",
    "stylelint-config-standard": "38.0.0",
    "stylelint-prettier": "5.0.3",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-plugin-pwa": "0.21.2",
    "vue-eslint-parser": "10.2.0"
  }
}
