{
  "private": true,
  "name": "ka-mensa-ui",
  "version": "0.0.0",
  "description": "Karlsruhe (KIT) Mensa meal plan UI for use with ka-mensa-api",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./config/build/webpack.prod.js",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.js,.vue ./src && stylelint ./src",
    "lint-fix": "eslint --fix --ignore-path .gitignore --ext .ts,.js,.vue ./src && stylelint --fix ./src",
    "test": "npm run lint",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./config/build/webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/ka-mensa-ui.git"
  },
  "keywords": [
    "karlsruhe",
    "mensa",
    "canteen",
    "user-interface"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/ka-mensa-ui/issues"
  },
  "homepage": "https://github.com/meyfa/ka-mensa-ui",
  "dependencies": {
    "@babel/runtime": "^7.13.9",
    "moment": "^2.29.1",
    "typeface-nunito": "^1.1.13",
    "vue": "^3.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@vue/compiler-sfc": "^3.2.2",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.21.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^36.0.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.16.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.2.0",
    "html-webpack-preconnect-plugin": "^1.2.0",
    "mini-css-extract-plugin": "^2.2.0",
    "style-loader": "^3.2.1",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^22.0.0",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "vue-eslint-parser": "^8.0.0",
    "vue-loader": "^16.5.0",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.2.0",
    "webpack-merge": "^5.7.3",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^6.1.1"
  }
}
