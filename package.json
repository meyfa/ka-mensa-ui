{
  "name": "ka-mensa-ui",
  "version": "0.0.0",
  "description": "Karlsruhe (KIT) Mensa meal plan UI for use with ka-mensa-api",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "lint": "eslint --ignore-path .gitignore --ext .js,.vue . && stylelint ./src",
    "lint-fix": "eslint --fix --ignore-path .gitignore --ext .js,.vue . && stylelint --fix ./src",
    "test": "npm run lint",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/ka-mensa-ui.git"
  },
  "keywords": [
    "karlsruhe",
    "mensa",
    "canteen",
    "user-interface"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/ka-mensa-ui/issues"
  },
  "homepage": "https://github.com/meyfa/ka-mensa-ui",
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "axios": "^0.19.2",
    "file-loader": "^5.0.2",
    "typeface-nunito": "^1.1.3",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-root-import": "^6.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
