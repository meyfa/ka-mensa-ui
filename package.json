{
  "private": true,
  "name": "ka-mensa-ui",
  "version": "0.0.0",
  "description": "Karlsruhe (KIT) Mensa meal plan UI for use with ka-mensa-api",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.js",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.js,.vue ./src && stylelint \"./src/**/*.{css,vue}\"",
    "lint-fix": "eslint --fix --ignore-path .gitignore --ext .ts,.js,.vue ./src && stylelint --fix \"./src/**/*.{css,vue}\"",
    "test": "npm run lint",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./webpack/webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/ka-mensa-ui.git"
  },
  "keywords": [
    "karlsruhe",
    "mensa",
    "canteen",
    "user-interface"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/ka-mensa-ui/issues"
  },
  "homepage": "https://github.com/meyfa/ka-mensa-ui",
  "dependencies": {
    "moment": "2.29.1",
    "typeface-nunito": "1.1.13",
    "vue": "3.2.29"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@vue/compiler-sfc": "3.2.29",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.5.1",
    "css-minimizer-webpack-plugin": "3.4.1",
    "eslint": "7.32.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "37.7.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-vue": "8.4.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "html-webpack-preconnect-plugin": "1.2.1",
    "mini-css-extract-plugin": "2.5.3",
    "postcss-html": "1.3.0",
    "style-loader": "3.3.1",
    "stylelint": "14.3.0",
    "stylelint-config-standard-vue": "1.0.0",
    "ts-loader": "9.2.6",
    "typescript": "4.5.5",
    "vue-eslint-parser": "8.2.0",
    "vue-loader": "17.0.0",
    "webpack": "5.68.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4",
    "webpack-merge": "5.8.0",
    "webpack-pwa-manifest": "4.3.0",
    "workbox-webpack-plugin": "6.4.2"
  }
}
